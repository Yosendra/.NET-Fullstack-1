@model GigHub.Models.Gig
@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

<form>
    <div class="mb-3">
        @Html.LabelFor(m => m.Venue, new { @class = "form-label" })
        @Html.TextBoxFor(m => m.Venue, new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>